<%= f.error_messages %>

<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :status %><br />
  <%= f.select :status, ["approved","pending"] %>
</p>
<p>
  <%= f.submit "Save" %>
  <%= hidden_field_tag :original_page, h(params[:original_page] || params[:page]) %>
  <%= hidden_field_tag :from, h(params[:from]) %>
</p>
